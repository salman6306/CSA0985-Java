class user{
public static void main(String [ ] args){
double num1=10,num2=20;
char operator='*';
switch(operator) {
case'+' :
System.out.println(num1+num2);
break ;
case'-' :
System.out.println(num1-num2);
break ;
case'*' :
System.out.println(num1*num2);
break ;
case'/' :
System.out.println(num1/num2);
break ;
default :
System.out.println("invalid operator");
}
}
}
